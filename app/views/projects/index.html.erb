<div class="container">
<div class="row pushdown">
<% if can? :all, School %>
            <%=link_to "Add Project", new_project_path, class: "btn btn-primary" %>
            <% end %>
            </div>
            <div class="row">
<table class="table table-striped table-hover">
    <tbody>
    <tr class="pushdown">
    <th>Project Name</th>
    <th>DSA Number</th>
    <th>School</th>
    	
    </tr>
<% @projects.each do |project| %>
  
    <tr>
    <td><%= link_to project.project_name,(project) %></td>
    <td><%= project.dsa_number %></td>
    <td><%= project.school.school_name %></td>
  
    </tr>
     <% end %>
  </tbody>
  </table>
  </div>
 </div>
